# Production Environment Variables
# Copy this file to .env and update the values

# Redis Configuration
# Generate a secure password: openssl rand -base64 32
REDIS_PASSWORD=your_secure_redis_password_here

# CORS Configuration
# Add your production domain
CORS_ALLOWED_ORIGINS=https://tictactoe.vincxx.dev,http://tictactoe.vincxx.dev
WEBSOCKET_ALLOWED_ORIGINS=https://tictactoe.vincxx.dev,http://tictactoe.vincxx.dev

# Frontend URLs (configured via Nginx reverse proxy)
VITE_API_URL=https://tictactoe.vincxx.dev/api
VITE_WS_URL=https://tictactoe.vincxx.dev/ws

# Optional: Uncomment and configure if needed
# VITE_ENV=production
